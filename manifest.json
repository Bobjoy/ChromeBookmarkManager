{
    "name": "MyBookmarks",
    "description": "my bookmarks manager",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png"
    },

    "options_page": "options.html",

    "permissions": [
        "bookmarks"
    ],

    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}